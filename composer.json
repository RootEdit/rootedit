{
    "name": "rootedit/rootedit",
    "description": "PHP micro-framework",
    "keywords": ["microframework"],
    "homepage": "http://rootedit.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Flacher",
            "email": "romain@rootedit.com"
        }
    ],
    "require" :{
        "psr/log":"1.0.2",
        "psr/container":"1.0.0",
        "psr/http-message":"1.0.1"
    },
    "autoload": {
        "psr-4": {"": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}